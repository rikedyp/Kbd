include "%L"

# APL Prefix DK keyboard layout .XCompose file
# The file apl_prefix defines keyboard layouts which set the prefix "introducer" "dead" key for several international layouts.
# Pressing the prefix key <Multi_key> followed by another key produces glyphs as defined below.
# Use with apl_prefix -variant ,dkonehalf or -variant ,dkless

<Multi_key> <Multi_key> : "⋄"
<Multi_key> <space> : "`"

<Multi_key> <1> : "¨"
<Multi_key> <2> : "¯"
<Multi_key> <3> : "<"
<Multi_key> <4> : "≤"
<Multi_key> <5> : "="
<Multi_key> <6> : "≥"
<Multi_key> <7> : ">"
<Multi_key> <8> : "≠"
<Multi_key> <9> : "∨"
<Multi_key> <0> : "∧"
<Multi_key> <plus> : "×"
<Multi_key> <dead_acute> : "÷"

